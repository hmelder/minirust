{
    "name": "minirust",
    "version": "0.0.1",
    "description": "A minimal rust implementation written in TypeScript using ANTLR",
    "packageManager": "yarn@4.9.1",
    "type": "module",
    "main": "dist/index.js",
    "scripts": {
        "build": "rollup -c",
        "generate-parser": "antlr4ng -Dlanguage=TypeScript -visitor -o src/parser src/SimpleLang.g4"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/hmelder/minirust.git"
    },
    "author": "hmelder",
    "license": "MIT",
    "files": [
        "dist"
    ],
    "devDependencies": {
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.2",
        "@types/node": "^22.14.1",
        "antlr4ng": "^3.0.16",
        "antlr4ng-cli": "^2.0.0",
        "conductor": "https://github.com/source-academy/conductor.git#0.2.1",
        "rollup": "^4.39.0",
        "tslib": "^2.8.1",
        "typescript": "^5.8.3"
    }
}
